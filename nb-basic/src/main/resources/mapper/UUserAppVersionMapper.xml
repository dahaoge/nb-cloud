<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hao.nb.cloud.basic.mapper.UUserAppVersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.hao.nb.cloud.basic.entity.UUserAppVersion">
        <id column="user_app_id" property="userAppId"/>
        <result column="version" property="version"/>
        <result column="create_by" property="createBy"/>
        <result column="create_time" property="createTime"/>
        <result column="update_by" property="updateBy"/>
        <result column="update_time" property="updateTime"/>
        <result column="deleted" property="deleted"/>
        <result column="user_id" property="userId"/>
        <result column="app" property="app"/>
        <result column="app_platform" property="appPlatform"/>
        <result column="app_version" property="appVersion"/>
        <result column="app_version_type" property="appVersionType"/>
        <result column="version_num" property="versionNum"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            version, create_by, create_time, update_by, update_time, deleted, user_app_id, user_id, app, app_platform, app_version, app_version_type, version_num
        </sql>


    <select id="pageMapData" parameterType="map" resultType="map">
        select
        <include refid="Base_Column_List"/>
        from u_user_app_version
    </select>
</mapper>
